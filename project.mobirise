            "<script>\n  document.addEventListener(\"DOMContentLoaded\", function () {\n    console.log('Init carousel indicator'); // <--- Dòng này\n\n    const carousel = document.querySelector(\".carousel\");\n    const indicator = carousel.querySelector(\".slide-position-indicator\");\n    const items = carousel.querySelectorAll(\".carousel-item\");\n\n    if (!carousel || !indicator || items.length === 0) {\n      console.log(\"Carousel, indicator hoặc items không tồn tại\");\n      return;\n    }\n\n    function updateIndicator() {\n      const activeIndex = Array.from(items).findIndex(item => item.classList.contains(\"active\"));\n      console.log(`Slide index updated: ${activeIndex + 1}/${items.length}`);\n      indicator.textContent = `${activeIndex + 1}/${items.length}`;\n    }\n\n    const observer = new MutationObserver(updateIndicator);\n    items.forEach(item => {\n      observer.observe(item, { attributes: true, attributeFilter: ['class'] });\n    });\n\n    updateIndicator();\n  });\n</script>"
